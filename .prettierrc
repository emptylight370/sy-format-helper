{
  "tabWidth": 2,
  "printWidth": 120,
  "singleQuote": true,
  "plugins": ["prettier-plugin-toml"],
  "overrides": [
    {
      "files": ["*.scss", "*.ts"],
      "options": {
        "tabWidth": 4
      }
    },
    {
      "files": [".vscode/*"],
      "options": {
        "tabWidth": 4,
        "singleQuote": false
      }
    }
  ]
}
